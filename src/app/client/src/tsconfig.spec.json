{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/spec",
    "baseUrl": "./",
    "paths": {
      "moment": [
        "../node_modules/moment/min/moment.min.js"
      ],
      "CsLibInitializer": [
        "app/service/CsLibInitializer/cs-lib-initializer.service.ts"
      ],
      "LazzyLoadScriptService": [
        "app/service/LazzyLoadScript/lazzy-load-script.service.ts"
      ],
      "@sunbird/test-util": [
        "app/testUtil/"
      ]
    },
    "types": [
      "jasmine",
      "node"
    ]
  },
  "files": [
    "test.ts",
    "polyfills.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
