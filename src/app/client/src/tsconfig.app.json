{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "experimentalDecorators": true,
    "baseUrl": "./",
    "paths": {
      "moment": [
        "../node_modules/moment/min/moment.min.js"
      ],
      "CsLibInitializer": [
        "app/service/CsLibInitializer/cs-lib-initializer.service.ts"
      ],
      "LazzyLoadScriptService": [
        "app/service/LazzyLoadScript/lazzy-load-script.service.ts"
      ],
      "sunbird-collection-editor": [
        "../node_modules/@project-sunbird/sunbird-collection-editor"
      ],
      "sunbird-collection-editor/*": [
        "../node_modules/@project-sunbird/sunbird-collection-editor/*"
      ]
    },
    "types": []
  },
  "files": [
    "main.ts",
    "polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts",
    "./*.d.ts"
  ]
}
